{
  "latex-workshop.latex.outDir": "./OUTPUT",
  "latex-workshop.latex.recipe.default": "xelatex -> biber -> makeglossaries -> xelatex -> xelatex",
  "latex-workshop.latex.recipes": [
    {
      "name": "xelatex -> biber -> makeglossaries -> xelatex -> xelatex",
      "tools": ["xelatex", "biber", "makeglossaries", "xelatex", "xelatex"]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "xelatex",
      "command": "xelatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-output-directory=./OUTPUT",
        "%DOC%"
      ]
    },
    {
      "name": "biber",
      "command": "biber",
      "args": [
        "--input-directory=./OUTPUT",
        "--output-directory=./OUTPUT",
        "%DOCFILE%"
      ]
    },
    {
      "name": "makeglossaries",
      "command": "makeglossaries",
      "args": ["-d", "./OUTPUT", "%DOCFILE%"]
    }
  ]
}
